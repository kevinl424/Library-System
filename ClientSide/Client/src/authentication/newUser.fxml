<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="515.0" prefWidth="361.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="authentication.NewUserController">
   <children>
      <Label layoutX="87.0" layoutY="25.0" text="Create New Account">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="83.0" layoutY="116.0" prefHeight="25.0" prefWidth="190.0" promptText="enter new username" />
      <TextField fx:id="password" layoutX="83.0" layoutY="225.0" prefHeight="25.0" prefWidth="190.0" promptText="enter new password" />
      <Label layoutX="80.0" layoutY="99.0" text="New Username:" />
      <Label layoutX="82.0" layoutY="208.0" text="New Password:" />
      <Label layoutX="85.0" layoutY="258.0" text="Retype Password:" />
      <TextField fx:id="rePassword" layoutX="83.0" layoutY="275.0" prefHeight="25.0" prefWidth="190.0" promptText="retype new password" />
      <Button fx:id="create" layoutX="153.0" layoutY="453.0" mnemonicParsing="false" onAction="#createAccount" text="Create" />
      <Label fx:id="error" layoutX="86.0" layoutY="405.0" prefHeight="38.0" prefWidth="190.0" textAlignment="CENTER" textFill="RED" wrapText="true" />
      <Hyperlink fx:id="returner" layoutX="14.0" layoutY="478.0" onAction="#returnOnClick" text="Return to Login" />
      <Label layoutX="82.0" layoutY="155.0" prefHeight="17.0" prefWidth="33.0" text="Email" />
      <TextField fx:id="email" layoutX="83.0" layoutY="172.0" prefHeight="25.0" prefWidth="190.0" promptText="enter email" />
      <TextField fx:id="verifyField" layoutX="88.0" layoutY="347.0" prefHeight="38.0" prefWidth="149.0" promptText="enter verification code" />
      <Button fx:id="verifyButton" layoutX="250.0" layoutY="354.0" mnemonicParsing="false" onAction="#verify" text="Verify" />
   </children>
</AnchorPane>
