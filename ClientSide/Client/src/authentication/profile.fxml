<AnchorPane prefHeight="504.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="authentication.ProfileController">
   <children>
      <Label fx:id="name" layoutX="14.0" layoutY="14.0" prefHeight="75.0" prefWidth="562.0" text="Name">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <Separator layoutX="-10.0" layoutY="90.0" prefHeight="8.0" prefWidth="792.0" />
      <Label layoutX="14.0" layoutY="110.0" text="Current books checked out:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ScrollPane layoutX="14.0" layoutY="146.0" prefHeight="346.0" prefWidth="372.0">
         <content>
            <VBox fx:id="checkoutVBox" prefHeight="346.0" prefWidth="371.0" />
         </content>
      </ScrollPane>
      <ScrollPane hbarPolicy="NEVER" layoutX="400.0" layoutY="146.0" prefHeight="346.0" prefWidth="372.0">
         <content>
            <VBox fx:id="reviewVBox" prefHeight="344.0" prefWidth="370.0" />
         </content>
      </ScrollPane>
      <Label layoutX="400.0" layoutY="110.0" prefHeight="30.0" prefWidth="155.0" text="User Reviews:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Hyperlink fx:id="returns" layoutX="670.0" layoutY="29.0" onAction="#returnToCat" text="Return to Catalog" />
   </children>
</AnchorPane>
